{
    "name": "fabcon",
    "service": "fabcon-python",
    "workspaceFolder": "/workspaces/mastering-open-mirroring",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "settings": {
        "python.pythonPath": "/usr/local/bin/python",
        "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker",
        "ms-mssql.mssql",
        "dvirtz.parquet-viewer",
        "GitHub.copilot",
        "GitHub.copilot-chat"
    ],
    "postCreateCommand": "/bin/bash /docker_init/init.sh"
}
